{
  "name": "sourcemorpher",
  "publisher": "rajatsharma",
  "displayName": "sourcemorpher",
  "description": "Transform code using JSCodeShift",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sourcemorpher.sortSpecifiedImports"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sourcemorpher.sortSpecifiedImports",
        "title": "Sourcemorpher: Sort specified imports"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "package": "vsce package",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jscodeshift": "^0.7.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.20.1",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vsce": "^1.77.0",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "jscodeshift": "^0.10.0",
    "jscodeshift-choose-parser": "^2.0.0"
  }
}
